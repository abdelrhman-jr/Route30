                });
                $('#from-val > span').text('EGP ' + $("#slider-range").slider("values", 0));
                $('#to-val > span').text('EGP ' + $("#slider-range").slider("values", 1));
                $('#slider-range a:first').addClass('first_item');
                $('#slider-range a:last').addClass('last_item');
                $('.quickView').click(function () {
                    $.fancybox.open('#em-popup', {
                        minWidth: '1050',
                        height: '90%',
                        //openEffect: 'fade',
                        closeEffect: 'elastic'
                    });
                    var link = $(this).attr('href');
                    $("#em-popup").load(link);
                });
                $('a.like').click(function () {
                    $this = $(this);
                    var data_id = $(this).parent().parent().attr('id');
                    $.fancybox.showLoading();
                    $.ajax({
                        type: 'POST',
                        url: '/product/favorite',
                        data: {'id': data_id},
                        success: function (result) {
                            obj = JSON.parse(result);
                            if (obj.value !== 1) {
                                $this.children('i').removeClass('fa-heart').addClass('fa-heart-o');
                                $.growl.error({title: "", message: "Like Removed!"});
                                $.fancybox.hideLoading();

                            } else {
                                $this.children('i').removeClass('fa-heart-o').addClass('fa-heart');
                                $.growl.notice({title: "", message: "You liked this product!"});
                                $.fancybox.hideLoading();
                            }
                        }
                    });
                });
                function collectQty() {
                    prices = {};
                    $('.PriceAndQuantity').each(function () {
                        var data_id = $(this).data('id');
                        var data_value = $(this).val();
                        if (data_value > 0) {
                            prices[data_id] = data_value;
                        }
                    });
                    return prices;
                }

                function addCart() {
                    $.fancybox.showLoading();
                    $.ajax({
                        type: 'POST',
                        url: '/e-commerce/cart/add-item-ajax',
                        data: {'data': JSON.stringify(prices)},
                        success: function (result) {
                            obj = JSON.parse(result);
                            if (obj.error == 0) {
                                if (obj.products.length > 0) {
                                    ga('require', 'ec');
                                    $.each(obj.products, function (i, product) {
                                        ga('ec:addProduct', {
                                            'id': product.id,
                                            'name': product.name,
                                            'category': product.category,
                                            'price': product.price,
                                            'quantity': product.qty
                                        });
                                    });
                                    ga('ec:setAction', 'add');
                                    ga('send', 'event', 'UX', 'click', 'add to cart');     // Send data using an event.
                                }

                                $.fancybox.hideLoading();
                                $('.fancybox-overlay').fadeOut();
                                $('.PriceAndQuantity').empty();
                                $('body').removeClass('fancybox-lock');
                                $('.cartWidgetContainer').empty().append(obj.html);
                                $.growl.notice({title: "", message: 'Added to your cart successfully.'});
                            } else if (obj.error == 1) {
                                $.fancybox.hideLoading();
                                $.growl.error({title: "", message: obj.message});
                            } else {
                                addCart();
                                $('body').removeClass('fancybox-lock');
                            }
                        }
                    });
                }
                $('#em-popup').delegate('.addToCart', 'click', function () {
                    prices = collectQty();
                    var value = $('.PriceAndQuantity').filter(function () {
                        return this.value != '';
                    });
                    if (value.length > 0) {
                        addCart();
                        value.val('');
                    } else {
                        $('.errorAlert').show();
                    }
                });
                $('#em-popup').delegate('.PriceAndQuantity', 'click', function () {
                    $('.errorAlert').hide();
                });
                $('#em-popup').delegate('.addToGiftList', 'click', function () {
                    var value = $('.PriceAndQuantity').filter(function () {
                        return this.value != '';
                    });
                    if (value.length > 0) {
                        $('.fancybox-overlay').fadeOut();
                        $('.lightboxContainer, .lightboxAlert').fadeIn();
                    } else {
                        $('.errorAlert').show();
                    }
                });
                $('.removeGiftList').click(function () {
                    $('.lightboxContainer, .lightboxAlert').fadeOut();
                });
                $('.addToGiftList').click(function () {
                    prices = collectQty();
                    var slug = $('.activeGiftList:checked').data('slug');
                    $.fancybox.showLoading();
                    $.ajax({
                        type: 'POST',
                        url: '/e-commerce/gift-list/add-item/ajax',
                        data: {'data': JSON.stringify(prices), 'slug': slug},
                        success: function (result) {
                            obj = JSON.parse(result);
                            if (obj.error == 1) {
                                $.fancybox.hideLoading();
                                $('body').removeClass('fancybox-lock');
                                $('.lightboxContainer, .lightboxAlert').fadeOut();
                                $.growl.error({title: "", message: obj.message});
                            } else {
                                $.fancybox.hideLoading();
                                $('.PriceAndQuantity').empty();
                                $('body').removeClass('fancybox-lock');
                                $('.lightboxContainer, .lightboxAlert').fadeOut();
                                $.growl.notice({title: "", message: obj.message});
                            }
                        }
                    });
                });
                $('.cancel').click(function () {
                    $('.lightboxContainer, .lightboxAlertSuccess').fadeOut();
                });
                $('#em-popup').delegate('.notify', 'click', function () {
                    $(this).next().children('.lightboxAlertNotify').fadeIn();
                    $(this).next('.lightboxNotifyContainer').fadeIn();
                });
                $('#em-popup').delegate('.cancel', 'click', function () {
                    $('.lightboxNotifyContainer, .lightboxAlertNotify').fadeOut();
                });
                $('#em-popup').delegate('.notifyBtn', 'click', function () {
                    $this = $(this);
                    var price_id = $('.notifyEmail').parent().parent().prev('.notify').data('id');
                    var mails = $this.parent().find('input.notifyEmail').val();
                    if ($.trim(mails).length != 0) {
                        $.fancybox.showLoading();
                        $.ajax({
                            type: 'POST',
                            url: '/product/notify-me',
                            data: {'id': price_id, 'email': mails},
                            success: function (result) {
                                obj = JSON.parse(result);
                                if (obj.error == 1) {
                                    $.fancybox.hideLoading();
                                    $('.lightboxNotifyContainer, .lightboxAlertNotify').fadeOut();
                                    $.growl.error({title: "", message: obj.message});
                                } else {
                                    $.fancybox.hideLoading();
                                    $('.lightboxNotifyContainer, .lightboxAlertNotify').fadeOut();
                                    $.growl.notice({title: "", message: obj.message});
                                }
                            }
                        });
                    } else {
                        $('.notifyEmail').parent().find('.errorAlert').show();
                    }
                });
                $('#em-popup').delegate('.notifyEmail', 'click', function () {
                    $(this).next('.errorAlert').hide();
                });

                $('.sortby').change(function () {
                    url = $('.sortby').find(':selected').data('url');
                    window.location.href = url;
                });
            });
    </script></body></html>